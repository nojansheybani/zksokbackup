#     zokrates verify && \
#     end_sec=$(date +%s) && \
#     end_nsec=$(date +%N) && \
#     duration_sec=$((end_sec - start_sec)) && \
#     duration_nsec=$((end_nsec - start_nsec)) && \
#     # Handle cases where nanoseconds wrap around
#     if [ "$duration_nsec" -lt 0 ]; then \
#         duration_sec=$((duration_sec - 1)); \
#         duration_nsec=$((1000000000 + duration_nsec)); \
#     fi && \
#     duration_ms=$((duration_nsec / 1000000)) && \
#     echo "Verify time: ${duration_sec}.${duration_ms} seconds"

COPY src/sha256.zok sha256.zok
RUN zokrates compile -i sha256.zok
RUN start_sec=$(date +%s) && \
    start_nsec=$(date +%N) && \
    zokrates setup --proving-scheme gm17 && \
    end_sec=$(date +%s) && \
    end_nsec=$(date +%N) && \
    duration_sec=$((end_sec - start_sec)) && \
    duration_nsec=$((end_nsec - start_nsec)) && \
    # Handle cases where nanoseconds wrap around
    if [ "$duration_nsec" -lt 0 ]; then \
        duration_sec=$((duration_sec - 1)); \
        duration_nsec=$((1000000000 + duration_nsec)); \
    fi && \
    duration_ms=$((duration_nsec / 1000000)) && \
    echo "Setup time: ${duration_sec}.${duration_ms} seconds"
RUN zokrates compute-witness -a 1 2 3 4 --verbose
RUN start_sec=$(date +%s) && \
    start_nsec=$(date +%N) && \
    zokrates generate-proof --proving-scheme gm17 && \
    end_sec=$(date +%s) && \
    end_nsec=$(date +%N) && \
    duration_sec=$((end_sec - start_sec)) && \
    duration_nsec=$((end_nsec - start_nsec)) && \
    # Handle cases where nanoseconds wrap around
    if [ "$duration_nsec" -lt 0 ]; then \
        duration_sec=$((duration_sec - 1)); \
        duration_nsec=$((1000000000 + duration_nsec)); \
    fi && \
    duration_ms=$((duration_nsec / 1000000)) && \
    echo "Proof time: ${duration_sec}.${duration_ms} seconds"
RUN start_sec=$(date +%s) && \
    start_nsec=$(date +%N) && \
    zokrates verify && \
    end_sec=$(date +%s) && \
    end_nsec=$(date +%N) && \
    duration_sec=$((end_sec - start_sec)) && \
    duration_nsec=$((end_nsec - start_nsec)) && \
    # Handle cases where nanoseconds wrap around
    if [ "$duration_nsec" -lt 0 ]; then \
        duration_sec=$((duration_sec - 1)); \
        duration_nsec=$((1000000000 + duration_nsec)); \
    fi && \
    duration_ms=$((duration_nsec / 1000000)) && \
    echo "Verify time: ${duration_sec}.${duration_ms} seconds"
